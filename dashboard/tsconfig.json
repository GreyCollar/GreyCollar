{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "preserve",
    "moduleResolution": "bundler",
    "target": "esnext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noEmit": true
  }
}
