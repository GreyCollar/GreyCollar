{
  "compilerOptions": {
    "baseUrl": "..",
    "paths": {
      "@/*": ["./*"]
    },
    "target": "es5",
    "lib": ["es5", "dom"],
    "sourceMap": true,
    "types": ["cypress", "node"],
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": true
  },
  "include": ["**/*.ts"]
}
