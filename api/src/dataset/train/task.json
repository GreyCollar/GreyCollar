    "id": "61c38f41-4c0b-4506-b4c8-abf9abc04f65",
    "description": "Get employee ID, scan hardware barcode, retrieve status message.",
    "steps": [
      {
        "action": "POSTGRES:query",
        "parameters": {
          "query": "SELECT * FROM employees WHERE id = {employeeId}"
        }
      },
      {
        "action": "POSTGRES:query",
        "parameters": {
          "query": "SELECT * FROM hardware WHERE barcode = {hardwareBarcode}"
        }
      },
      {
        "action": "POSTGRES:query",
        "parameters": {
          "query": "SELECT * FROM hardware_status WHERE hardware_id = {hardwareBarcode}"
        }
      },
      {
        "action": "JIRA:create",
        "parameters": {
          "projectKey": "IT",
          "summary": "Hardware {hardwareBarcode} status is ORANGE",
          "description": "Employee ID: {employeeId}, Hardware Barcode: {hardwareBarcode}, Status: ORANGE"
        }
      }
    ]
  },
  {
    "id": "0cbfbecd-5e0d-4924-b5fd-6de262285936",
    "description": "Check product availability, if not exist, create order.",
    "steps": [
      {
        "action": "GREY:check_product_count",
        "parameters": {
          "productId": "{productId}"
        }
      },
      {
        "action": "PLATFORM:decision",
        "parameters": {
          "condition": "response <= 0",
          "true": [
            "GREY:create_order"
          ],
          "false": [
            "GREY:reduce_product_count"
          ]
        }
      },
      {
        "action": "GREY:create_order",
        "parameters": {
          "productId": "{productId}",
          "quantity": 10
        }
      },
      {
        "action": "GREY:reduce_product_count",
        "parameters": {
          "productId": "{productId}"
        }
      }
    ]
  }
]